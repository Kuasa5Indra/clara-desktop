<basic:MyPage  xmlns:basic="clr-namespace:Velacro.UIElements.Basic;assembly=Velacro" x:Class="CLARA_Desktop.Login.LoginPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CLARA_Desktop.Login"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Login Page">

    <Grid>
        <TextBox x:Name="email_textBox" HorizontalAlignment="Left" Height="23" Margin="268,158,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="274" AutomationProperties.HelpText="Email"/>
        <PasswordBox x:Name="password_box" HorizontalAlignment="Left" Margin="268,205,0,0" VerticalAlignment="Top" Width="274" Height="23" AutomationProperties.HelpText="password"/>
        <Button x:Name="login_button" Content="Login" HorizontalAlignment="Left" Margin="370,259,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</basic:MyPage>
